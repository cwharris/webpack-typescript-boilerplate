{
  "name": "@cwharris/webpack-typescript-boilerplate",
  "version": "0.0.0-development",
  "license": "ISC",
  "author": {
    "name": "Christopher Harris",
    "url": "http://cwharris.com"
  },
  "scripts": {
    "dev:c": "npm run build:dev:main && concurrently \"webpack-dev-server --inline --hot --config webpack.config.renderer.dev.ts\" \"electron ./app-dist/main.js\"",
    "dev-other": "webpack-dev-server --open --config webpack.config.renderer.dev.ts",
    "build:dev": "npm run build:dev:main && npm run build:dev:renderer",
    "build:dev:main": "webpack --config webpack.config.main.dev.ts",
    "build:dev:renderer": "webpack --config webpack.config.renderer.dev.ts",
    "build:prod": "npm run build:prod:main && npm run build:prod:renderer",
    "build:prod:main": "webpack --config webpack.config.main.prod.ts",
    "build:prod:renderer": "webpack --config webpack.config.renderer.prod.ts",
    "postinstall": "npm install ./app"
  },
  "dependencies": {
    "@types/html-webpack-plugin": "^2.11.2",
    "@types/webpack": "^2.2.5",
    "@types/webpack-merge": "^0.0.4",
    "concurrently": "^3.3.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1",
    "webpack-dev-server": "^2.3.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.6"
  }
}
