{
  "name": "@cwharris/webpack-typescript-boilerplate",
  "version": "0.0.0-development",
  "license": "ISC",
  "author": {
    "name": "Christopher Harris",
    "url": "http://cwharris.com"
  },
  "scripts": {
    "postinstall": "npm install ./app",
    "dev": "concurrently --kill-others \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "webpack --watch --config webpack.config.main.dev.ts",
    "dev:renderer": "webpack-dev-server --inline --config webpack.config.renderer.dev.ts",
    "build": "npm run build:prod:main && npm run build:prod:renderer",
    "build:main": "webpack --config webpack.config.main.prod.ts",
    "build:renderer": "webpack --config webpack.config.renderer.prod.ts"
  },
  "dependencies": {
    "@types/electron": "^1.4.32",
    "@types/html-webpack-plugin": "^2.11.2",
    "@types/webpack": "^2.2.5",
    "@types/webpack-merge": "^0.0.4",
    "electron": "^1.4.15",
    "concurrently": "^3.3.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1",
    "webpack-dev-server": "^2.3.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.1.0",
    "typescript": "^2.1.6"
  }
}
